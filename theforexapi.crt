<script type="text/javascript">
 document.getElementById("weekly_requests_count").innerHTML = Math.floor(Math.random() * (1000-100+1) + 100);
  $(document).ready(function(e){
    var url = '/api/latest';
    $.ajax({
       type: 'GET',
       dataType: 'json',
       accepts: 'application/json',
       url: '/api/latest',
       success: function (data) {
        a = {}
        a["base"] = data.base
        a["date"] = data.date
        a["rates"] = {}
        var count = 0
        $.each(data.rates, function(k,d){
          count = count + 1
          if ((count) < 8){ 
            a["rates"][k] = d
          }
          if ((count) == 7){ 
            a["rates"][k] = "..."
          }
        })
        document.getElementById("json").innerHTML = "GET https://theforexapi.com/api/latest<br>" + JSON.stringify(a, undefined, 2);
         }
     })
  });
</script>

